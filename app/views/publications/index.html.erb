<h1>Mes tweets</h1>
<table>
    <tr>
        <th>Content:</th>
    </tr>
    <% @publications.each do |publication| %>
    <tr>
        <td> <%= publication.content %> </td>
        <td> <%= link_to 'Modifier ce tweet', edit_publication_path(publication.id)%> </td>
        <td> <%= link_to 'Supprimer ce tweet', publication_path(publication.id), method: :delete, data: {confirm: "Supprimer ce tweet ?"}  %> </td> 
    </tr>
    <% end %>
</table>
